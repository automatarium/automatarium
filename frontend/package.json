{
  "name": "frontend",
  "version": "1.0.0",
  "schemaVersion": "1.0.0",
  "private": true,
  "scripts": {
    "start": "yarn prebuild && parcel public/index.html --no-cache",
    "copy:locales": "cp -r public/locales dist",
    "prebuild": "rm -rf dist && mkdir dist && yarn copy:locales",
    "build": "yarn prebuild && parcel build public/index.html && yarn postbuild",
    "postbuild": "node build-sw.js",
    "serve": "serve -s dist -l 3000",
    "check:types": "tsc --noemit"
  },
