    "axios": "^0.30.0",
    "csstype": "^3.1.3",
    "dayjs": "^1.11.2",
    "eslint": "^9.28.0",
    "goober": "^2.1.8",
    "i18next": "^23.2.3",
    "i18next-http-backend": "^3.0.2",
    "immer": "^9.0.12",
    "lodash.clonedeep": "^4.5.0",
    "lodash.groupby": "^4.6.0",
    "lucide-react": "^0.17.9",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-a11y-dialog": "^6.1.5",
