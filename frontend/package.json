{
  "name": "frontend",
  "version": "1.0.0",
  "schemaVersion": "1.0.0",
  "private": true,
  "scripts": {
    "start": "yarn prebuild && parcel public/index.html --no-cache",
    "copy:locales": "cp -r public/locales dist",
    "prebuild": "rm -rf dist && mkdir dist && yarn copy:locales",
    "build": "yarn prebuild && parcel build public/index.html",
    "lint": "eslint . --ext .js,.ts,.tsx --cache --cache-location '../.eslintcache'",
    "lint:fix": "yarn lint --fix",
    "check:types": "tsc --noemit"
  },
