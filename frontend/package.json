    "axios": "^0.30.0",
    "csstype": "^3.1.3",
    "dayjs": "^1.11.13",
    "globals": "^16.2.0",
    "goober": "^2.1.16",
    "i18next": "^23.16.8",
    "i18next-http-backend": "^3.0.2",
    "immer": "^9.0.21",
    "lodash.clonedeep": "^4.5.0",
    "lodash.groupby": "^4.6.0",
    "lucide-react": "^0.515.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
